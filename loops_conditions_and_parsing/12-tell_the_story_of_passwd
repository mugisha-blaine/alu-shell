#!/usr/bin/env bash
#Bash script that displays the content of the file /etc/passwd, using the while loop + IFS
while IFS=: read -r f1 f2 f3 f4 f5
do
      echo "The user $f1 is part of the $f3 gang, lives in $f4 and rides $f5. $f3's place is protected by the passcode $f2, more info about the user here: $f1"
done < /etc/passwd 
